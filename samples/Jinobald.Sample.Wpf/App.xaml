<jino:WpfApplicationBase
    x:Class="Jinobald.Sample.Wpf.App"
    x:TypeArguments="views:MainWindow, views:SplashScreenWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:jino="clr-namespace:Jinobald.Wpf.Application;assembly=Jinobald.Wpf"
    xmlns:views="clr-namespace:Jinobald.Sample.Wpf.Views">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Jinobald.Wpf;component/Themes/Generic.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</jino:WpfApplicationBase>
